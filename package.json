{
  "author": "Troy Alford",
  "bugs": "https://github.com/TroyAlford/react-jsx-parser/issues",
  "description": "A React component which can parse JSX and output rendered React Components",
  "keywords": [
    "react",
    "jsx"
  ],
  "license": "MIT",
  "main": "lib/cjs/react-jsx-parser.min.js",
  "name": "react-jsx-parser",
  "repository": "TroyAlford/react-jsx-parser",
  "typings": "lib/types/index.d.ts",
  "version": "1.25.0",
  "dependencies": {
    "acorn": "^7.2.0",
    "acorn-jsx": "^5.2.0",
    "core-js": "^3.6.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.10.1",
    "@types/acorn": "^4.0.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "jest": "^25.4.0",
    "jest-cli": "^25.4.0",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "mkdirp": "^1.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "source-map-explorer": "^2.4.2",
    "terser-webpack-plugin": "^3.0.2",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "resolutions": {
    "extend": "^3.0.2",
    "merge": "^1.2.1"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "postversion": "git push origin HEAD && git push origin HEAD --tags",
    "prebuild": "mkdirp ./lib && rm -rf ./lib/*",
    "postbuild": "yarn tsc --project ./tsconfig.json",
    "preversion": "yarn test",
    "sourcemap": "yarn build && source-map-explorer ./lib/react-jsx-parser.min.js",
    "test": "yarn jest",
    "version": "yarn build && git add -A ./lib"
  },
  "jest": {
    "collectCoverageFrom": [
      "source/**/*.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>setupTests.js"
    ],
    "transform": {
      ".js$": "babel-jest"
    }
  }
}
