{
  "author": "Troy Alford",
  "bugs": "https://github.com/TroyAlford/react-jsx-parser/issues",
  "description": "A React component which can parse JSX and output rendered React Components",
  "files": [
    "lib/"
  ],
  "keywords": [
    "react",
    "jsx"
  ],
  "license": "MIT",
  "main": "lib/cjs/react-jsx-parser.min.js",
  "name": "react-jsx-parser",
  "repository": "TroyAlford/react-jsx-parser",
  "typings": "lib/react-jsx-parser.d.ts",
  "version": "1.26.0",
  "dependencies": {
    "@types/jsdom": "^16.2.4",
    "acorn": "^8.0.4",
    "acorn-jsx": "^5.3.1",
    "core-js": "^3.6.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/runtime-corejs3": "^7.12.1",
    "@types/acorn": "^4.0.5",
    "@types/enzyme": "^3.10.7",
    "@types/jest": "^26.0.14",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.5.2",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "jest": "^26.5.3",
    "jest-cli": "^26.5.3",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "mkdirp": "^1.0.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "source-map-explorer": "^2.5.0",
    "terser-webpack-plugin": "^5.0.0",
    "typescript": "^4.0.3",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.0.0"
  },
  "peerDependencies": {
    "react": "^16",
    "react-dom": "^16"
  },
  "resolutions": {
    "extend": "^3.0.2",
    "merge": "^1.2.1"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "postversion": "git push origin HEAD && git push origin HEAD --tags",
    "prebuild": "mkdirp ./lib && rm -rf ./lib/*",
    "postbuild": "cp ./source/types/react-jsx-parser.d.ts ./lib/react-jsx-parser.d.ts",
    "preversion": "yarn test",
    "sourcemap": "yarn build && source-map-explorer ./lib/es5/react-jsx-parser.min.js",
    "test": "yarn jest",
    "version": "yarn build && git add -A ./lib"
  },
  "jest": {
    "collectCoverageFrom": [
      "source/**/*.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>setupTests.js"
    ],
    "transform": {
      ".js$": "babel-jest"
    }
  }
}
